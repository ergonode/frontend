{
  "name": "ergonode-front",
  "version": "1.3.0",
  "description": "Ergonode - Desktop PWA Ready Product Information Management Platform",
  "keywords": [
    "ergonode",
    "PIM"
  ],
  "author": "Ergonode team",
  "private": true,
  "bin": {
    "cli": "./lib/index.js"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 5.0.0"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "analyze": "nuxt build --analyze",
    "start": "nuxt start",
    "lint:scss": "stylelint './**/*.scss' './**/*.vue'",
    "lint": "eslint --ext .js,.vue,.json . && npm run lint:scss",
    "lint:dev": "npm run lint -- --cache && npm run lint:scss",
    "test": "jest --no-cache",
    "test:dev": "npm run test -- --coverage --u",
    "env": "./lib/index.js env",
    "info": "./lib/index.js info",
    "modules": "./lib/index.js modules",
    "modules:all": "./lib/index.js modules-all",
    "modules:relations": "./lib/index.js modules-relations",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:cc": "cypress cache clear",
    "cypress:ci": "cypress run --record --group 4x-electron --parallel",
    "publish:all": "lerna publish ${semver} --force-publish --no-push --no-changelog",
    "list:modules": "lerna la",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
